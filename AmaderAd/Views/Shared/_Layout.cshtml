<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title - Amader Ad</title>
    @Html.Partial("_LayoutHead")
</head>
<body>
    <div class="container body-content" style="background-color:#fff">
        @Html.Partial("_TopMenu")
        @RenderBody()   
    </div>
    <div class="container">
        @Html.Partial("_Footer")
    </div>

    @Html.Partial("_LayoutFooter")
    @RenderSection("scripts", required: false)
</body>
</html>
